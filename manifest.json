{
  "manifest_version": 3,
  "name": "Console Log Estensione",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://translate.google.com/*",
        "https://translate.google.it/*",
        "https://translate.google.de/*",
        "https://translate.google.es/*",
        "https://translate.google.fr/*",
        "https://translate.google.co.uk/*"
      ],
      "js": ["content.js"]
    }
  ],
  "host_permissions": [
    "https://translate.google.com/*",
    "https://translate.google.it/*",
    "https://translate.google.de/*",
    "https://translate.google.es/*",
    "https://translate.google.fr/*",
    "https://translate.google.co.uk/*"
  ]
}
